---
//layout
import Layout from "src/layouts/Layout.astro";
//swiper
import 'swiper/css/bundle';
//style
import "@styles/scss/pages/Home.scss"

//compontents
import Hero from "@components/hero/Hero.astro";
import Services from "@components/Home/Services.astro";
import Category from "@components/Home/Category.astro";
import NewPro from "@components/Home/NewPro.astro";
import ViewModel from "@components/ui/ViewModel.astro";
import ProCate from "@components/Home/ProCate.astro";
import SectionBreak from "@components/ui/SectionBreak.astro";
import ProCateNoOffre from "@components/Home/ProCateNoOffer.astro";
import Brand from "@components/Home/Brand.astro";
import BlogSection from "@components/Home/BlogSection.astro";
//
import Img1 from "@assets/images/banner/break/banner-1.jpg";
import Img2 from "@assets/images/banner/break/banner-2.jpg";
import Img3 from "@assets/images/banner/break/banner-6.jpg";
import Img4 from "@assets/images/banner/break/banner-sm-1.jpg";

import Img5 from "@assets/images/banner/break/banner-4.jpg";
import Img6 from "@assets/images/banner/break/banner-5.jpg";
import Img7 from "@assets/images/banner/break/banner-5.jpg"
import Img8 from "@assets/images/banner/break/banner-4.jpg"

---

<Layout
  title="Ryn E-commerce | الصفحة الرئيسية"
  des=""
>
  <Hero/>
  <Services/>
  <Category/>
  <NewPro/>
<ViewModel/>
<SectionBreak img={{img1:Img1,img2:Img2,img3:Img3}} alt="break" href="#!" titleImg="اشتري الان" flex={false} />
<ProCate/>
<SectionBreak img={{img1:Img4,img2:Img5,img3:Img6 , img4:Img7,img5:Img8}} alt="break" href="#!" titleImg="اشتري الان" flex={true}/>
<ProCateNoOffre/>
<Brand/>
<BlogSection/>
</Layout>
<script src="../scripts/home.ts"></script>
